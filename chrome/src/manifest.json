{
  "name": "Copy Fixer - Personal Version",
  "version": "1.0",
  "description": "Copy title and URL using Control/Command-C; URL using Command-Option-C.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png"
   },
   "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*"],
      "js": ["copyfixer.js"],
      "run_at": "document_end"
    }
  ],
  "background_page": "background.html"
}
